    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "humps": "^2.0.1",
    "mini-css-extract-plugin": "^2.7.0",
    "mjml": "^4.13.0",
